{
  "name": "tazeai",
  "version": "1.0.0",
  "description": "Dashboard Inteligente para Investidores da B3",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && .\\venv\\Scripts\\python main.py",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && .\\venv\\Scripts\\pip install -r requirements.txt",
    "install:all": "npm run install:frontend && npm run install:backend",
    "build:frontend": "cd frontend && npm run build"
  },
  "keywords": ["b3", "investimentos", "ai", "dashboard", "financeiro"],
  "author": "",
  "license": "MIT"
}

